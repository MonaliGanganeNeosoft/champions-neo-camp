{
  "name": "Champions",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^16.0.0",
    "@ckeditor/ckeditor5-react": "^2.1.0",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "material-ui-pickers": "^1.0.0-rc.13",
    "moment": "^2.22.2",
    "node-sass": "^4.9.1",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-email-editor": "^1.0.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "scriptjs": "^2.5.8"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-js": "react-scripts start",
    "start": "set HTTPS=true && npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/scss -o public/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/scss -o public/css --watch --recursive"
  }
}
